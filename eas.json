{
  "cli": {
    "appVersionSource": "remote",
    "version": ">= 3.9.0"
  },
  "build": {
    "development-simulator": {
      "env": {
        "NODE_ENV": "development"
      },
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "large",
        "simulator": true
      }
    },
    "development": {
      "env": {
        "NODE_ENV": "development"
      },
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "large"
      },
      "channel": "development"
    },
    "preview": {
      "env": {
        "NODE_ENV": "test"
      },
      "distribution": "internal",
      "ios": {
        "resourceClass": "large"
      },
      "channel": "preview",
      "autoIncrement": true
    },
    "production": {
      "env": {
        "NODE_ENV": "production"
      },
      "ios": {
        "resourceClass": "large"
      },
      "channel": "production",
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "ios":{
        "appName": "xLog - On-Chain Blogging",
        "ascAppId": "6449499296",
        "appleId": "caspian.zhao@outlook.com"
      }
    }
  }
}
