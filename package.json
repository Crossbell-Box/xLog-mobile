{
  "name": "xlog",
  "version": "0.0.1",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "NODE_ENV=development expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "release": "changeset status & changeset version",
    "publish:updates": "eas update --platform all --non-interactive --auto",
    "build:development": "NODE_ENV=development eas build --platform all --non-interactive --profile development",
    "build:preview": "eas build --platform all --non-interactive --profile preview",
    "build:production": "pnpm run release & eas build --platform all --non-interactive --profile production --auto-submit",
    "generate-env-types:common": "dotenv-types-generator -f ./.env.common -m",
    "generate-env-types:env": "dotenv-types-generator -f ./.env.development -m",
    "generate-env-types": "run-s generate-env-types:common generate-env-types:env"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.1",
    "@types/react": "~18.0.27",
    "dotenv": "^16.0.3",
    "expo": "~48.0.10",
    "expo-haptics": "~12.2.1",
    "expo-status-bar": "~1.4.4",
    "expo-updates": "~0.16.4",
    "react": "18.2.0",
    "react-native": "0.71.6",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-progress": "^5.0.0",
    "react-native-reanimated": "~2.14.4",
    "react-native-redash": "^18.1.0",
    "react-native-safe-area-context": "4.5.0",
    "react-native-svg": "13.4.0",
    "react-native-webview": "11.26.0",
    "typescript": "^4.9.4",
    "expo-splash-screen": "~0.18.1",
    "expo-dev-client": "~2.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "dotenv-types-generator": "^1.1.2",
    "npm-run-all": "^4.1.5"
  },
  "private": true
}
