    "start": "NODE_ENV=development expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "release": "changeset status & changeset version",
    "ios:publish:updates": "eas update --platform ios --non-interactive --auto",
