    "ios": "expo run:ios",
    "web": "expo start --web",
    "release:version": "changeset status & changeset version",
    "release:publish": "changeset tag",
    "update:preview": "eas update --non-interactive --channel preview --auto",
    "build:development": "eas build --non-interactive --profile development",
    "build:preview": "eas build --non-interactive --profile preview",
    "build:production": "yarn run release & eas build --non-interactive --profile production --auto-submit",
    "generate-env-types:common": "dotenv-types-generator -f ./.env.common -m",
    "generate-env-types:env": "dotenv-types-generator -f ./.env.development -m",
    "generate-env-types": "run-s generate-env-types:common",
    "prebuild": "expo prebuild --clean",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "patch-package && node_modules/.bin/rn-nodeify --install crypto --hack && node_modules/.bin/rn-nodeify --install stream --hack",
    "prepare": "husky install"
  },
