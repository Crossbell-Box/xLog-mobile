    "ios:build:preview": "eas build --platform ios --non-interactive --profile preview",
    "ios:build:production": "pnpm run release & eas build --platform ios --non-interactive --profile production --auto-submit",
    "generate-env-types:common": "dotenv-types-generator -f ./.env.common -m",
    "generate-env-types:env": "dotenv-types-generator -f ./.env.development -m",
    "generate-env-types": "run-s generate-env-types:common",
    "test": "vitest"
  },
